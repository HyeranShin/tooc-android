<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff">


    <ImageView
        android:id="@+id/iv_storeimage_myreview"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toStartOf="@+id/guideline37"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/background_dark" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06666667" />

    <TextView
        android:id="@+id/tv_name_myreview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/kopub_medium"
        android:text="롭스 홍대2호점"
        android:textColor="#262626"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/iv_storeimage_myreview"
        app:layout_constraintTop_toTopOf="@+id/iv_storeimage_myreview" />

    <TextView
        android:id="@+id/tv_area_myreview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/kopub_medium"
        android:text="서울시 마포구 샬라샬라"
        android:textColor="#707070"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/tv_name_myreview"
        app:layout_constraintTop_toBottomOf="@+id/tv_name_myreview" />

    <TextView
        android:id="@+id/tv_ymd_myreview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/kopub_medium"
        android:text="2018.12.23"
        android:textColor="#707070"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/tv_area_myreview"
        app:layout_constraintTop_toBottomOf="@+id/tv_area_myreview" />

