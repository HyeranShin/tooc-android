                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toBottomOf="@+id/tv_name_mypage" />

            <ImageView
                android:id="@+id/iv_profile_mypage"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_marginEnd="8dp"
                android:src="@drawable/mypage_bt_default"
                app:layout_constraintBottom_toBottomOf="@+id/textView26"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toTopOf="@+id/tv_name_mypage" />

            <TextView
                android:id="@+id/tv_current_bag_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="76dp"
                android:fontFamily="@font/kopub_bold"
                android:text="03"
                android:textColor="#262626"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toBottomOf="@+id/textView26" />

            <TextView
                android:id="@+id/tv_review_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/kopub_bold"
                android:text="12"
                android:textColor="#262626"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/textView44"
                app:layout_constraintStart_toStartOf="@+id/textView44" />

            <TextView
                android:id="@+id/tv_resevable_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/kopub_bold"
                android:text="On"
                android:textColor="#262626"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/textView45"
                app:layout_constraintStart_toStartOf="@+id/textView45" />

            <TextView
                android:id="@+id/textView43"
