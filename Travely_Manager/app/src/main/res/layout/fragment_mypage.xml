                app:layout_constraintBottom_toBottomOf="@+id/textView43"
                app:layout_constraintEnd_toEndOf="@+id/textView43"
                app:layout_constraintStart_toStartOf="@+id/tv_current_bag_mypage"
                app:layout_constraintTop_toTopOf="@+id/tv_current_bag_mypage">

