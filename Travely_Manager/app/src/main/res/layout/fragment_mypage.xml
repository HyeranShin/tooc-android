                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:fontFamily="@font/kopub_medium"
                android:text="가게 리뷰"
                android:textColor="#494949"
                android:textSize="12sp"
